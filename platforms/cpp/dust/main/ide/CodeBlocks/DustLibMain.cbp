<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="DustLibMain" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="$(#ark_debug.LIB)/DustLibMain" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="../obj/Debug/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add directory="../../../api/src" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="$(#ark_release.LIB)/DustLibMain" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="../obj/Release/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add directory="$(#ark_debug.INCLUDE)" />
			<Add directory="$(#ark_debug.BASE)/dust/runtime/src" />
		</Compiler>
		<Linker>
			<Add library="$(#ark_debug.LIB)/libDustLibApi.a" />
			<Add library="$(#ark_debug.LIB)/libDustLibRuntime.a" />
		</Linker>
		<Unit filename="../../src/DustMain.cpp" />
		<Unit filename="../../src/DustMain.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
